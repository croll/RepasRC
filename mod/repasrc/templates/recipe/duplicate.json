{
	"method" : "post",
	"id" : "informations",
	"action" : "/recette/edition/duplicate",
	"class" : "form-horizontal",
	"elements" : 
	[
		{	"type" : "text",
			"name" : "label",
		  "id"   : "recipeDuplicationLabel",
			"validators" : 
			[ 
				"required"
			]
		},
		{	"type" : "button",
			"name" : "submit",
			"onclick" : "duplicateRecipe()",
			"class" : "btn btn-primary",
			"value" : "Dupliquer la recette"
		},
		{	"type" : "button",
			"name" : "cancel",
			"class" : "btn",
			"value" : "Abandonner la duplication"
		}
	]
}
